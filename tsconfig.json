{
  "compilerOptions": {
    "types": ["node"],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/country-profiles/[slug]/page.tsxx", "app/components/country-profile.js", "app/country-profiles/page.tsx", "app/country-profiles/[country]/page.js", "app/components/leaderboard.tsx", "app/components/back-button.js", "app/components/select-box.js"],
  "exclude": ["node_modules"]
}
